<h1>Login</h1>
<p>Find me in app/views/pages/signup.html.erb</p>
<%= form_with url: login_path, local: true, method: :post do |f| %>
  <div>
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div>
    <%= f.label :password %>
    <%= f.text_field :password %>
  </div>
  <div>
    <%= f.submit 'Log in.' %>
  </div>
<% end %>
